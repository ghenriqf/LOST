{
  "version": 2,
  "routes": [
    {
      "src": "/Build/(.*)\\.br",
      "headers": {
        "Content-Encoding": "br"
      },
      "dest": "/Build/$1.br"
    },
    {
      "src": "/Build/(.*)\\.gz",
      "headers": {
        "Content-Encoding": "gzip"
      },
      "dest": "/Build/$1.gz"
    }
  ]
}
