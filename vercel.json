{
  "version": 2,
  "builds": [
    {
      "src": "Build/Build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Build/(.*)\\.js\\.br",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/javascript"
      },
      "dest": "/Build/$1.js.br"
    },
    {
      "src": "/Build/(.*)\\.wasm\\.br",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/wasm"
      },
      "dest": "/Build/$1.wasm.br"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}